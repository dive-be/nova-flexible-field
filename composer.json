{
    "name": "dive-be/nova-linkable-field",
    "description": "This package allows you to provide a field that can optionally link to a different model as an alternative to a manual attribute value.",
    "keywords": [
        "laravel",
        "nova",
        "model"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Dive\\LinkableField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dive\\LinkableField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
